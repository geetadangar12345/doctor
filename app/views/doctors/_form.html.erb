<%= form_with(model: doctor, local: true) do |f| %>
<table border="1">
	<tr>
		<h3><%= Wek.first.name%></h3>
		<%= f.select :wek_id , options_for_select(@wek.collect{|w|[w.name, w.id]},{includ_blank: 'select day'})%>
			doctor name<%= f.text_field :name %>
			<p>on leave<%=f.check_box :not_available %></p>
			<%= f.hidden_field :user_id, value: current_user.id%>
		<td>
			<p>select start time</p>
				<%= f.select :start_time, ['10:00', '15:00']%>
		</td>
	</tr>
	<tr>
	<td>
			<p>select end time</p>
			<%= f.select :end_time, ['13:00', '19:00']%>
		</td>
	</tr>
	<tr>
	<tr>
		<td>
			<%= submit_tag	 'submit'%>
		</td>
	</tr>
	</table>
<%end%>